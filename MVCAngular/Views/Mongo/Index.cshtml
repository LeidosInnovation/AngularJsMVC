@model IEnumerable<MVCAngular.Models.Person>

@{
    ViewBag.Title = "Index";
}

<h2>Show all</h2>
    <div class="row" >
        <div class="col-md-12">
             <table class="table table-striped">
                <tr>
                    
                    <th >ID</th>
                    <th>FirstName</th>

                    <th>Last Name</th>

                </tr>
                 @{
                     foreach( var p in Model)
                     {
                         <tr>
                             <td>@p.Id</td>
                             <td>@p.FirstName </td>
                             <td>@p.LastName </td>
                         </tr>
                     }
                 }

                 </table>
            </div>
        </div>